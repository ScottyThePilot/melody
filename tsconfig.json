{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@config": ["src/config"],
      "@fs/*": ["src/fs/*"],
      "@core/*": ["src/core/*"],
      "@utils/*": ["src/utils/*", "src/utils/struct/*"]
    }
  }
}
